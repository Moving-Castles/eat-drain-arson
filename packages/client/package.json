{
  "name": "client",
  "version": "0.2.0",
  "private": "true",
  "scripts": {
    "test": "jest",
    "lint": "eslint . --fix --ext .ts",
    "prettier": "prettier --write '**/*.{ts,tsx}'",
    "// use yarn dev when developing with a locally linked version of MUD": "",
    "dev": "vite --force",
    "// use yarn start when developing with MUD packages from npm or yalc": "",
    "start": "vite --port 3000",
    "// start": "rimraf ../../.parcel-cache && rimraf .parcel-cache && yarn parcel index.html --port 3000",
    "// note: must use MUD packages from npm or yalc to build for production": "",
    "build": "rimraf dist && rimraf ../../.parcel-cache && rimraf .parcel-cache && parcel build index.html",
    "prod": "yarn serve dist",
    "check:circdepen": "dpdm ./src/index.ts"
  },
  "staticFiles": {
    "staticPath": "src/public"
  },
